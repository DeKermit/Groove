<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#0E0E0E</color>
    <color name="colour_primary">#C2E538</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">Groove</string>
    <style name="CustomFontStyle">
        <item name="android:typeface">@font/plus_jakarta_sans_300</item>
    </style>
    <style name="Theme.Groove" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->

        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@android:color/transparent</item>
        <!-- Customize your theme here. -->
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>

    </style>
</resources>